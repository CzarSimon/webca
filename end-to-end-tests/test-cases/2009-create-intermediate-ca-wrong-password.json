{
  "name": "Create INTERMEDIATE_CA RSA certificate with incorrect signatory password",
  "request": {
    "method": "POST",
    "path": "/api/v1/certificates",
    "headers": {
      "Authorization": "Bearer ${admin.token}"
    },
    "body": {
      "name": "api-test intermediate ca",
      "subject": {
        "commonName": "api-test intermediate ca"
      },
      "type": "INTERMEDIATE_CA",
      "algorithm": "RSA",
      "password": "9c5c615ea50b5702fd705d40684cbb21",
      "options": {
        "keySize": 1024
      },
      "signatory": {
        "id": "${rootCA.id}",
        "password": "this-is-the-wrong-password"
      }
    }
  },
  "response": {
    "status": 500
  }
}
